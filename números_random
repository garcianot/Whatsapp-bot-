import pyautogui
import webbrowser as web
from time import sleep
import random

numero_ramdon=['***********','***********']
link1='https://web.whatsapp.com/send?phone=+'
link2='+?text='
n=(random.choice(numero_ramdon))
numero = link1 + link2 + n

web.open(numero)
sleep(15)
pyautogui.click(x=774, y=810)
sleep(3)
with open('*****', 'r') as file:
    for line in file:
        pyautogui.typewrite(line)
        pyautogui.press('enter')
